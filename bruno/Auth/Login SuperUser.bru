meta {
  name: Login SuperUser
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/Auth/login
  body: json
  auth: none
}

body:json {
  {
    "username": "SuperUser",
    "password": "superUser"
  }
}

script:post-response {
  if (res.status === 200) {
    bru.setVar("token", res.body.token);
    bru.setVar("refreshToken", res.body.refreshToken);
  }
}
